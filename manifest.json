{
  "name": "Web3 Checker",
  "version": "0.1",
  "description": "Simple extension to check if a webpage is Web 3.0 enabled",
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "permissions": ["storage"],
  "background": {
    "scripts": ["eventPage.js"],
    "persistent": false
  },
  "manifest_version": 2
}